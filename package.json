{
  "name": "vuex-reflect",
  "homepage": "http://github.com/bprinty/vuex-reflect",
  "repository": "http://github.com/bprinty/vuex-reflect",
  "version": "0.1.0",
  "authors": [
    "Blake Printy <bprinty@gmail.com> (http://bprinty.github.io)"
  ],
  "description": "Data reflection for Vuex store using API or GraphQL",
  "license": "Apache-2.0",
  "keywords": [],
  "ignore": [
    "**/.*",
    "node_modules",
    "tests",
    "src",
    "Makefile",
    "package-lock.json"
  ],
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --watch",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs:api": "vuepress-jsdoc --source ./src/ --dist docs --folder api --title API --exclude *.test.js,exclude.js",
    "docs": "yarn docs:api && vuepress build docs",
    "publish": "npm publish"
  },
  "main": [
    "dist/vuex-reflect.min.js"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/register": "^7.8.3",
    "axios": "^0.19.2",
    "core-js": "^2.6.5",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.5.5",
    "@vuepress/theme-vue": "^1.3.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "import-local": "^3.0.1",
    "mermaid": "^8.4.7",
    "mocha": "^6.1.4",
    "mocha-phantomjs": "^4.0.2",
    "vuepress": "^1.2.0",
    "vuepress-jsdoc": "^2.0.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.10"
  }
}
